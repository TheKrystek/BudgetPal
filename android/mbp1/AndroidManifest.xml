        <activity
            android:name="com.mbp1.Budzet"
            android:label="@string/title_activity_budzet" >
        </activity>
        <activity
            android:name="com.mbp1.EdytujBudzet"
            android:label="@string/title_activity_edytuj_budzet" >
        </activity>
        <activity
            android:name="com.mbp1.Stats"
            android:label="@string/title_activity_stats" >
        </activity>
        <activity
            android:name="com.mbp1.DodajWydatek"
            android:label="@string/title_activity_dodaj_wydatek" >
